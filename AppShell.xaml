<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="InteractiveRatingComponent.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:InteractiveRatingComponent.Pages"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        ContentTemplate="{DataTemplate pages:RatingPage}"
        Route="RatingPage" />

    <ShellContent
        ContentTemplate="{DataTemplate pages:SubmittedRatingPage}"
        Route="SubmittedRatingPage" />

</Shell>
